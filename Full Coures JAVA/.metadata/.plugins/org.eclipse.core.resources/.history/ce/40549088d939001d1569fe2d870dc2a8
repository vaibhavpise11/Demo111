import java.util.Scanner;

//Q 2 Java program to print sum of  three float numbers

public class sum {

	public static void main(String[] args) {
		int a,b,c;
		System.out.println("Enter any two decimal numbers for addition...");
		
		Scanner s = new Scanner (System.in);
		
		a=s.nextInt();
		b=s.nextInt();
		System.out.println("sum of two number is...");
		c=a+b;
		
		
	}

}
