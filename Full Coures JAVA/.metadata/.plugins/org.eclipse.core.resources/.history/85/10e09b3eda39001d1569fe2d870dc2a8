import java.util.Scanner;

//Q 2 Java program to print sum of  three float numbers

public class sum {

	public static void main(String[] args) {
		float a,b,c;
		System.out.println("Enter any two decimal numbers for addition...");
		
		Scanner s = new Scanner (System.in);
		
		a=s.nextFloat();
		b=s.nextFloat();
		c=a+b;
		System.out.println("sum of three decimal number is= "+c);

		
	}

}
