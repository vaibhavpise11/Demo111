import java.util.Scanner;

public class else_if {
	public static void main(String[] args) {
		int marks;
		System.out.printf("Enter your marks....");
		Scanner obj= new Scanner (System.in);
		marks = obj.nextInt();
		if(marks>=40 && marks<=60) {
			System.out.println("Congratulations have passed the examination.");	
		}
				
	else if(marks>=60 && marks>=100) {
		System.out.println("Congratulations have passed the examination with distinction.");		
	}
		else if(marks<40 && marks>=0){
		System.out.println("Sorry you are failed .... Try again");
	}
	
}

}
