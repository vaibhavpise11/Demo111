import java.util.Scanner;

public class Addition {

	public static void main(String[] args) {
		int a,b,c;
		
		System.out.print("Enter any two Numbers.....");
		
		Scanner d=new Scanner (System.in);
		
		a=d.nextInt();
		b=d.nextInt();
		c=a+b;
		
		System.out.print("Sum of two Numbers is.......");
		
				

	}

}
