import java.util.Scanner;

public class Float_addition {

	public static void main(String[] args) {
		float a,b,c,d;
		
		System.out.print("Enter any three  Numbers.....");
		
		Scanner e=new Scanner (System.in);
		
		a=e.nextFloat();
		b=e.nextFloat();
		c=e.nextFloat();
		d= a+b+c ;
		
		System.out.print("Sum of Three Numbers is......." +d );
	}
				

	}
